    "n = int(input(\"Enter number\"))\n",
    "h = len(str(n))\n",
    "s = 0\n",
    "i = n\n",
    "while i > 0:\n",
    "    mod= i%10\n",
    "    s = s + mod**h\n",
    "    i = i//10\n",
    "if (s == n):\n",
    "    print(\"Armstrong\")\n",
    "else:\n",
    "    print(\"not Armstrong\")"
